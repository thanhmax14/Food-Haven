                                    {
                                        foreach (var item in Model.itemCheck)
                                        {

                                            <tr>
                                                <td class="image product-thumbnail"><img src="@item.ItemImage" alt="#"></td>
                                                <td>
                                                    <h6 class="w-160 mb-5"><a href="/Home/ProductDetail/@item.productID" class="text-heading">@item.ItemName</a></h6>
