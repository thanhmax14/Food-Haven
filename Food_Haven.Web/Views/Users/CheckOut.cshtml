                        <button id="submitPayment" type="button" class="btn btn-fill-out btn-block mt-30">
                            Place an Order <i class="fi-rs-sign-out ml-15"></i>
                        </button>
                    </div>

                    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
                    <script>
                        $(document).ready(function () {
                            $("#submitPayment").click(function () {
                                var selectedOption = $("input[name='payment_option']:checked").val();
                                if (!selectedOption) {
                                                           new Notify({
                            status: 'error',
                            title: 'Failed',
                            text: 'Please select a payment method!',
                            effect: 'fade',
                            speed: 300,
                            showIcon: true,
                            showCloseButton: true,
                            autoclose: true,
                            autotimeout: 3000,
                            position: 'right top'
                        });
                        return;

                                }

                                var token = $('input[name="__RequestVerificationToken"]').val();
                                var url = '@Url.Action("SubmitPayment", "Users")';
                                var dataToSend = {
                                    firstName: $('#firstName').val(),
                                    lastName: $('#lastName').val(),
                                    email: $('#email').val(),
                                    phone: $('#phone').val(),
                                    address: $('#address').val(),
                                    note: $('#note').val(),
                                    paymentOption: selectedOption
                                };

                                $("#submitPayment").html('<i class="fa fa-spinner fa-spin"></i> Processing...').prop('disabled', true);

                                $.ajax({
                                    url: url,
                                    type: 'POST',
                                    headers: { "RequestVerificationToken": token },
                                    data: dataToSend,
                                    success: function (response) {
                                        if (response.haveUrl){
                                            window.location.href = response.redirectUrl;
                                        }else{
                                                                                          if (response.success) {
                          
                            document.getElementById("preloader-active").style.display = "block";
                            setTimeout(function () {
                                window.location.href = "/Users#orders";
                            }, 1000);
                        }
                         else {
                            new Notify({
                                status: 'error',
                                title: 'Failed',
                                text: response.msg || 'Submission failed!',
                                effect: 'fade',
                                speed: 300,
                                showIcon: true,
                                showCloseButton: true,
                                autoclose: true,
                                autotimeout: 3000,
                                position: 'right top'
                            });
                        }

                                        }
                                        
                                        $("#submitPayment").html('Place an Order').prop('disabled', false);
                                    },
                                    error: function (xhr) {
                                                               new Notify({
                            status: 'error',
                            title: 'Failed',
                            text: 'Purchase failed, please contact the admin!',
                            effect: 'fade',
                            speed: 300,
                            showIcon: true,
                            showCloseButton: true,
                            autoclose: true,
                            autotimeout: 3000,
                            position: 'right top'
                        });

                                        $("#submitPayment").html('Place an Order').prop('disabled', false);
                                    }
                                });
                            });
                        });
                    </script>

                </div>
