                                                            <div class="product-rating" style="width:90%">
                                                            </div>
                                                        </div>
                                                      @*   <span class="font-small ml-5 text-muted"> (4.0)</span> *@
                                                    </div>
                                                </td>
                                                <td>
                                                    <h6 class="text-muted pl-20 pr-20">x @item.ItemQuantity</h6>
                                                </td>
                                                <td>
                                                    <h4 class="text-brand">₫@item.ItemPrice</h4>
                                                </td>
                                            </tr>
                                        }
                                    }







                                </tbody>
                            </table>
