                                        const filterText = filterTextElement.textContent.toLowerCase().trim();
                                        const periodMap = {
                                            'today': 'today',
                                            'yesterday': 'yesterday',
                                            'last 7 days': 'last7days',
                                            'last 30 days': 'last30days',
                                            'this month': 'thismonth',
                                            'last month': 'lastmonth'
                                        };
                                        return periodMap[filterText] || 'today';
                                    }

