    <script>
        $(document).ready(function () {
            // Auto height + ẩn lỗi khi gõ vào textarea
            $('#resolutionNote').on('input', function () {
                $('#noteError').addClass('d-none');
                this.style.height = 'auto';
                this.style.height = (this.scrollHeight) + 'px';
            });

            // Ẩn lỗi khi chọn lại hành động
            $('#resolutionAction').on('change', function () {
                if ($(this).val()) {
                    $('#actionError').addClass('d-none');
                }
            });

            $('#btnResolveSubmit').on('click', function (e) {
                e.preventDefault();

                // Reset lỗi
                $('#actionError').addClass('d-none');
                $('#noteError').addClass('d-none');

                const btn = $(this);
                const spinner = btn.find('.spinner-border');
                const btnText = btn.find('.btn-text');

                const complaintId = $('#complaintId').val();
                const action = $('#resolutionAction').val();
                const note = $('#resolutionNote').val().trim();

                let hasError = false;

                if (!action) {
                    $('#actionError').removeClass('d-none');
                    hasError = true;
                }

                if (!note) {
                    $('#noteError').removeClass('d-none');
                    hasError = true;
                }

                if (!complaintId || hasError) {
                    // Hiển thị loading ngắn để tạo cảm giác phản hồi
                    btn.prop('disabled', true);
                    spinner.removeClass('d-none');
                    btnText.text('Đang xử lý...');

                    setTimeout(() => {
                        btn.prop('disabled', false);
                        spinner.addClass('d-none');
                        btnText.text('Xác nhận xử lý');
                    }, 300);

                    return;
                }

                // Gửi AJAX nếu hợp lệ
                btn.prop('disabled', true);
                spinner.removeClass('d-none');
                btnText.text('Đang xử lý...');

                $.ajax({
                    url: '/Admin/ResolveComplaint',
                    type: 'POST',
                    data: {
                        id: complaintId,
                        action: action,
                        note: note
                    },
                    headers: {
                        'RequestVerificationToken': $('input[name="__RequestVerificationToken"]').val()
                    },
                    success: function (response) {
                        if (response.success) {
                            Swal.fire("Thành công", "Đã xử lý khiếu nại!", "success").then(() => {
                                $('#resolveModal').modal('hide');
                                location.reload();
                            });
                        } else {
                            Swal.fire("Lỗi", response.message || "Xử lý thất bại!", "error");
                        }
                    },
                    error: function () {
                        Swal.fire("Lỗi", "Đã xảy ra lỗi khi gửi yêu cầu.", "error");
                    },
                    complete: function () {
                        btn.prop('disabled', false);
                        spinner.addClass('d-none');
                        btnText.text('Xác nhận xử lý');
                    }
                });
            });
        });
    </script>




